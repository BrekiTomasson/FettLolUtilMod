{
	"__comment": "Written by Mickey Joe Alpha",
    "multipart": [
        {   "apply": [
		    { "model": "block/pumpkin_top" },
            { "model": "block/pumpkin_top", "y": 90 },
            { "model": "block/pumpkin_top", "y": 180 },
            { "model": "block/pumpkin_top", "y": 270 }
			]
        },
        {   "apply": [
		    { "model": "block/pumpkin_bottom" },
            { "model": "block/pumpkin_bottom", "y": 90 },
            { "model": "block/pumpkin_bottom", "y": 180 },
            { "model": "block/pumpkin_bottom", "y": 270 }
			]
        },
		{     "when": { "facing": "north" },
              "apply": [
		    { "model": "block/jack_o_lantern" },
            { "model": "block/jack_o_lantern_0" }
			]
        },
		{     "when": { "facing": "south" },
              "apply": [
		    { "model": "block/jack_o_lantern", "y": 180 },
            { "model": "block/jack_o_lantern_0", "y": 180 }
			]
        },
		{     "when": { "facing": "west" },
              "apply": [
		    { "model": "block/jack_o_lantern", "y": 270 },
            { "model": "block/jack_o_lantern_0", "y": 270 }
			]
        },
		{     "when": { "facing": "east" },
              "apply": [
		    { "model": "block/jack_o_lantern", "y": 90 },
            { "model": "block/jack_o_lantern_0", "y": 90 }
			]
		}
    ]
}